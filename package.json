{
  "name": "playground",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "start": "vite",
    "play": "pnpm -C play dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "rmn": "pnpm recursive exec -- find . -name node_modules -type d -prune -exec rm -rf '{}' + && rm -rf node_modules",
    "rmp": "node src/scripts/rmp"
  },
